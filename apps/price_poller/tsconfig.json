{
  "compilerOptions": {
    "lib": ["ESNext"],
    "target": "ES2022",
    "module": "CommonJS",
    "moduleResolution": "node",
    "allowJs": false,
    "verbatimModuleSyntax": false,
    "noEmit": false,
    "outDir": "dist",
    "strict": true,
    "skipLibCheck": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": ["src", "seed.ts"]
}
